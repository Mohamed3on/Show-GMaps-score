{
  "manifest_version": 3,
  "name": "CreamCrop - Google Maps",

  "version": "1.0",
  "description": "Shows custom CreamCrop score for Google Maps places",
  "author": "Mohamed Oun",
  "content_scripts": [
    {
      "matches": ["*://*.google.com/maps/*", "*://*.google.com/maps*"],
      "js": ["content-script.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": ["*://*.google.com/maps/*"],
  "icons": {
    "48": "icon.png",
    "128": "icon.png",
    "512": "icon.png"
  }
}
